const a = 1;
const b = 3;
const c = 5;
const d = 7;
const e = 8.7;

let sum = 0;

if (Number.isInteger(a)) {
  sum = sum + a;
}
if (Number.isInteger(b)) {
  sum = sum + b;
}
if (Number.isInteger(c)) {
  sum = sum + c;
}
if (Number.isInteger(d)) {
  sum = sum + d;
}
if (Number.isInteger(e)) {
  sum = sum + e;
}
console.log(sum);
